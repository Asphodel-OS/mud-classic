{
  "name": "@mud-classic/phaserx",
  "author": "Asphodel Studios",
  "license": "MIT",
  "type": "module",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Asphodel-OS/mud-classic.git",
    "directory": "packages/phaserx"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "pnpm build",
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve --minify --clean --sourcemap --legacy-output",
    "dev": "tsup src/index.ts --format cjs,esm --dts-resolve --watch --sourcemap --legacy-output",
    "lint": "eslint . --ext .ts",
    "nuke": "rimraf node_modules/ dist/",
    "release": "pnpm run build && changeset publish",
    "test": "pnpm run build && jest --passWithNoTests --coverage"
  },
  "dependencies": {
    "@use-gesture/vanilla": "10.2.9",
    "mobx": "^6.4.2",
    "phaser": "3.60.0-beta.14",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@mud-classic/utils": "^0.0.1",
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "phaser": "3.60.0-beta.14"
  },
  "publishConfig": {
    "access": "public"
  }
}