{
  "name": "@mud-classic/utils",
  "author": "Asphodel Studios",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Asphodel-OS/mud-classic.git",
    "directory": "packages/utils"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts-resolve --minify --clean --sourcemap --legacy-output",
    "dev": "tsup src/index.ts --format cjs,esm --dts-resolve --watch --sourcemap --legacy-output",
    "release": "pnpm run build && changeset publish",
    "lint": "eslint . --ext .ts",
    "test": "tsc && jest"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "buffer-lite": "^1.0.0",
    "ethers": "^5.6.6",
    "jest": "^27.5.1",
    "mobx": "^6.5.0",
    "proxy-deep": "^3.1.1",
    "rxjs": "^7.5.5",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "tsup": "^8.0.1"
  },
  "dependencies": {
    "typedoc-plugin-markdown": "^3.13.6"
  },
  "publishConfig": {
    "access": "public"
  }
}